<div class="row">
    <div class="col-sm-9">
        <h1>Add a new restroom</h1>
        <%= form_for @restroom do |f| %>
            <%= render partial: 'form', locals: {restroom: @restroom, f: f} %>
            <%= f.submit "Add restroom", class: 'btn cta' %>
        <% end %>
    </div>
    <div class="col-sm-3">
        <%= render 'layouts/right_nav_links' %>
    </div>
</div>